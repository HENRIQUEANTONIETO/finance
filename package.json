{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npx dotenv-cli -e .env.development -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx dotenv-cli -e .env -- node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx dotenv-cli -e .env.test -- jest --runInBand",
    "test:unit": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.unit.config.ts",
    "test:int": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.int.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "npx dotenv-cli -e .env.test -- jest --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.e2e.config.ts",
    "prisma:generate": "dotenv -e .env -- npx prisma generate --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset --schema=./src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:studio:dev": "dotenv -e .env.development -- npx prisma studio --schema=./src/shared/infrastructure/database/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@nestjs/testing": "^11.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "dotenv-cli": "^10.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.14.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-fastify": "^11.1.3",
    "@prisma/client": "^6.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}